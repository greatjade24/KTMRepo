package Backjun;

import java.util.Scanner;

public class B1550 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		String num = "";
		int res;
		
		while (num.length() < 6) {

			num = sc.nextLine();
		}

		res = Integer.valueOf(num, 16);
		
		System.out.printf("%d", res);
	}

}
