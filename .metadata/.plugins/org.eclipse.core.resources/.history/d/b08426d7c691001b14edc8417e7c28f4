package Backjun;

import java.util.Arrays;
import java.util.Scanner;

public class B10818 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int num[] = new int[n];
		
		for(int i = 0; i < n; i++) {
			num[i] = sc.nextInt();
		}
		
		Arrays.sort(num);
		
		int min = num[0];
		int max = num[n-1];

		System.out.println(min + " " + max);
	}

}
