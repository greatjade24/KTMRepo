package Backjun;

import java.util.Scanner;

public class B10996 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		for(int repeat = 0; repeat < n; repeat++) {
			for(int i = 1; i <= n; i++) {
				if(i % 2 == 1)
					System.out.printf("*");
				if(i % 2 == 0)
					System.out.printf(" ");
				}
			System.out.println("");
			for(int j = 0; j < n; j++) {
				if(j % 2 == 1)
					System.out.printf("*");
				if(j % 2 == 0)
					System.out.printf(" ");
				}
			System.out.println("");
		}
	
		

	}

}
