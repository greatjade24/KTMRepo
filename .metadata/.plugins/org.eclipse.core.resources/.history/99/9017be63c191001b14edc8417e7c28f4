package Backjun;

import java.util.Scanner;

public class B8437 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		double apple = sc.nextDouble();
		double dif = sc.nextDouble();
		double a, b;

		if(apple % 2 != 0) {
			a = (apple/2) + (dif / 2);
			b = (apple/2) - (dif / 2);
		}else {
			a = apple/2 + dif/2;
			b = apple/2 - dif/2;
		}
		
		System.out.println((int)a + "\n" + (int)b);
	}

}
