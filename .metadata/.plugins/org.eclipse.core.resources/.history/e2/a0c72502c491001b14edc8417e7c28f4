package Backjun;

import java.util.Scanner;

public class B8437 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		long apple = sc.nextLong();
		long dif = sc.nextLong();
		long a, b;

		if(apple % 2 != 0) {
			a = (apple/2) + (dif / 2 + 1);
			b = (apple/2) - (dif / 2);
		}else {
			a = apple/2 + dif/2;
			b = apple/2 - dif/2;
		}
		
		System.out.println(a + "\n" + b);
	}

}
