package Backjun;

import java.util.Scanner;

public class B9498 {

	public static void main(String[] args) {

		int grade;
		
		Scanner s = new Scanner(System.in);
		grade = s.nextInt();
		
		if(grade >= 90)
			System.out.println("A");
		else if(grade >= 80)
			System.out.println("B");
		else if(grade >= 70)
			System.out.println("C");
		else if(grade >= 60)
			System.out.println("D");
		else
			System.out.println("F");
	

	}

}
